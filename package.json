{
  "name": "foodeli",
  "version": "1.0.0",
  "private": true,
  "description": "Food Delivery Page",
  "type": "module",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\"",
    "eslint": "eslint source/js/",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "build": "gulp buildProd",
    "start": "gulp"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/*.svg",
    "source/*.webmanifest",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@htmlacademy/editorconfig-cli": "^2.0.4",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "browser-sync": "^2.29.3",
    "del": "^7.0.0",
    "eslint": "^8.44.0",
    "eslint-config-htmlacademy": "^9.1.1",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-svgstore": "^9.0.0",
    "postcss": "^8.4.25",
    "postcss-scss": "^4.0.6",
    "posthtml-include": "^1.7.4",
    "sass": "^1.63.6",
    "stylelint": "^15.10.3",
    "stylelint-config-htmlacademy": "^2.0.4",
    "webpack": "^5.88.1",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "core-js": "^3.31.1"
  }
}
